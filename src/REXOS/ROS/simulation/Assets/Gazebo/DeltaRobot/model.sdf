<!-- ============================Start Joints========================================  --> 

<!-- =Joint= these connect links together useing joints to simulate motors  --> 

	<!-- connecting the base to a kind of hacked object to make it stay on its place --> 
	<joint type="prismatic" name="BaseToWorld">
		<pose>0 0 0 0 0 0</pose>
		<child>world</child>
		<parent>base</parent>
		<axis>
			<xyz>1 0 0</xyz>
			<limit>
				<upper>0.0</upper>
				<lower>0.0</lower>
			</limit>
			<dynamics>
				<damping>20</damping>
			</dynamics>
		</axis>
	</joint>

	<!-- connecting upper arms to respective motors --> 
	<joint type="revolute" name="upperArm1ToMotor1"><!-- upper arm 1 to motor 1--> 
		<pose>0 0 0 0 0 0</pose>
		<child>upperArm1</child>
		<parent>base</parent>
		<axis>
			<xyz>1 0 0</xyz>
			<limit>
				<upper>0</upper>
				<lower>0</lower>
			</limit>
			
		</axis>
	</joint>

